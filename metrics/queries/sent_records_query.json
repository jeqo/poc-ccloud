{
  "aggregations": [
      {
          "agg": "SUM",
          "metric": "io.confluent.kafka.server/sent_records"
      }
  ],
  "filter": {
      "filters": [
          {
              "field": "metric.label.cluster_id",
              "op": "EQ",
              "value": "lkc-75dop"
          }
      ],
      "op": "AND"
  },
  "granularity": "PT1M",
  "group_by": [
      "metric.label.topic"
  ],
  "intervals": [
      "2020-07-28T09:30:00+01:00/2020-07-28T10:30:00+01:00"
  ],
  "limit": 25
}